<map table="audience" result="audience_list_datamap">
  <column column="audience_id" name="object_id" type="INTEGER" size="11" key="PRI" order="15" format="audience%d"></column>
  <column column="audience_id" name="object_type" type="INTEGER" size="11" key="PRI" order="15" format="audience"></column>
  <column column="audience_id" name="object_name" type="INTEGER" size="11" key="PRI" order="15" format="audience%0d"></column>
  <column column="audience_paid_status" type="TINYINT" size="1" key="" order="1" format="%0d"></column>
  <column column="fk_screening_unique_key" name="audience_screening_unique_key" type="VARCHAR" size="255" key="" order="2"></column>
  <column column="fk_payment_id" name="audience_payment_id" type="INTEGER" size="11" key="" order="3" format="%0d"></column>
  <column column="audience_invite_code" type="VARCHAR" size="255" key="" order="4"></column>
  <column column="audience_ip_addr" type="VARCHAR" size="15" key="" order="5"></column>
  <column column="audience_created_at" type="TIMESTAMP" size="" key="" order="6" util="item::dateW3XMLIN"></column>
  <column column="audience_updated_at" type="TIMESTAMP" size="" key="" order="7" util="item::dateW3XMLIN"></column>
  <column column="audience_status" type="TINYINT" size="1" key="" order="8" format="%0d"></column>
  <column column="audience_username" type="VARCHAR" size="255" key="" order="9"></column>
  <column column="audience_review" type="LONGVARCHAR" size="" key="" order="10"></column>
  <column column="audience_review_status" type="LONGVARCHAR" size="" key="" order="10" format="%0d"></column>
  <column column="audience_short_url" type="VARCHAR" size="255" key="" order="11"></column>
  <column column="audience_ticket_price" type="FLOAT" size="" key="" order="12" format="%.02f"></column>
  <column column="fk_user_id" name="audience_user_id" type="INTEGER" size="11" order="13" format="%0d"></column>
  <column column="fk_screening_id" name="audience_screening_id" type="INTEGER" size="11" order="14"  format="%0d"></column>
  <column column="audience_id" type="INTEGER" size="11" key="PRI" order="15" format="%0d"></column>
  <join localtable="audience" local="fk_user_id" foreigntable="user" foreign="user_id" left="false">
    <foreigncolumn column="user_default_timezone" name="audience_screening_timezone_id" type="INTEGER" size="" key="" order="3" />
    <foreigncolumn column="user_email" name="audience_user_email" type="INTEGER" size="" key="" order="3" />
  </join>
  <join localtable="audience" local="fk_screening_id" foreigntable="screening" foreign="screening_id" left="true">
    <foreigncolumn column="screening_name" name="audience_screening_name" type="DATE" size="" key="" order="1" />
    <foreigncolumn column="screening_date" name="audience_screening_date" type="DATE" size="" key="" order="1" />
    <foreigncolumn column="screening_time" name="audience_screening_time" type="DATE" size="" key="" order="1" />
    <foreigncolumn column="screening_end_time" name="audience_screening_end_time" type="DATE" size="" key="" order="1" format="dateW3XMLIN" />
    <foreigncolumn column="fk_host_id" name="audience_screening_user_id" type="DATE" size="" key="" order="1" format="%0d" />
    <foreigncolumn column="screening_default_timezone_id" name="audience_user_timezone_id" type="DATE" size="" key="" order="1" format="" />
    <foreigncolumn column="screening_still_image" name="audience_screening_still_image" type="VARCHAR" size="" key="" order="1" format="" />
    <foreigncolumn column="screening_constellation_image" name="audience_screening_constellation_image" type="VARCHAR" size="" key="" order="1" format="" />
    <foreigncolumn column="screening_description" name="audience_screening_description" type="VARCHAR" size="" key="" order="1" format="" />
    <foreigncolumn column="screening_qa" name="audience_screening_qa" type="TEXT" size="" key="" order="1" format="" />
    <foreigncolumn column="screening_featured" name="audience_screening_featured" type="TINYINT" size="1" key="" order="10" format="%0d" />
    <foreigncolumn column="screening_is_dohbr" name="audience_screening_is_dohbr" type="TINYINT" size="1" key="" order="10" format="%0d" />
    <foreigncolumn column="screening_is_private" name="audience_screening_is_private" type="TINYINT" size="1" key="" order="" format="%0d" />

  </join>
  <join localtable="screening" local="fk_film_id" foreigntable="film" foreign="film_id" left="true">
    <foreigncolumn column="film_id" name="audience_film_id" type="VARCHAR" size="255" key="" order="5" format="%0d" />
    <foreigncolumn column="film_name" name="audience_film_name" type="VARCHAR" size="255" key="" order="5" />
    <foreigncolumn column="film_alt_name" name="audience_film_alt_name" type="VARCHAR" size="255" key="" order="5" />
    <foreigncolumn column="film_still_image" name="audience_film_still_image" type="VARCHAR" size="255" key="" order="5" />
    <foreigncolumn column="film_logo" name="audience_film_logo" type="VARCHAR" size="255" key="" order="5" />
    <foreigncolumn column="film_homelogo" name="audience_film_homelogo" type="VARCHAR" size="255" key="" order="5" />
    <foreigncolumn column="film_info" name="audience_film_info" type="VARCHAR" size="255" key="" order="5" />
  </join>
  <criterion limit="0" distinct="true">
    <criteria scope="PROCESS" value="audience_id" column="audience_id"/>
    <ascorderby column="audience_id"></ascorderby>
    <!-- <descorderby column="column_name" />
    <criteria value="0" column="column_name"/>--></criterion>
  <pagination>
    <pagesperpage value="5"></pagesperpage>
    <recordssperpage value="1"></recordssperpage>
    <page value="1" var="page"></page>
    <sort var="sort"></sort>
  </pagination>
</map>
