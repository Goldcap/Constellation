<?xml version="1.0" encoding="UTF-8"?>
<module name="login" baseurl="/modules/login/">
	<attributes>
    <assets>
	  </assets>
  </attributes>
	<content>
    <AFORM>
      <FORMNAME>Address</FORMNAME>
      <FORMACTION>$self</FORMACTION>
      <JSVALIDATE>TRUE</JSVALIDATE>
      <FORMCLASS/>
      <SERVERVALIDATE>TRUE</SERVERVALIDATE>
      <PASSTHROUGHVALUES>TRUE</PASSTHROUGHVALUES>
      <QUERYVALUES>TRUE</QUERYVALUES>
      <SHOWERRORS>FALSE</SHOWERRORS>
      <REQS>FALSE</REQS>
      <FORMELEMENTGROUP>
        <FORMELEMENT>
          <NAME>brochurehead</NAME>
          <DISPLAY>Modify Address</DISPLAY>
          <TYPE>display</TYPE>
          <DEFAULT/>
          <CLASS/>
          <VTIP/>
          <REQUIRED/>
          <DISPLAYCLASS>headline</DISPLAYCLASS>
          <SIZE/>
        </FORMELEMENT>
        <FORMELEMENT>
          <NAME>requirenotice</NAME>
          <DISPLAY>* REQUIRED FIELDS</DISPLAY>
          <TYPE>display</TYPE>
          <DEFAULT/>
          <CLASS/>
          <VTIP/>
          <REQUIRED/>
          <DISPLAYCLASS>reqmessage</DISPLAYCLASS>
          <SIZE/>
        </FORMELEMENT>
      </FORMELEMENTGROUP>
      <FORMELEMENTGROUP>
        <FORMELEMENT>
          <NAME>errornotice</NAME>
          <DISPLAY/>
          <TYPE>errornotice</TYPE>
          <DEFAULT>Please complete Fields outlined in red.</DEFAULT>
          <CLASS>errornotice</CLASS>
          <VTIP/>
          <REQUIRED/>
          <DISPLAYCLASS>col2</DISPLAYCLASS>
          <SIZE/>
        </FORMELEMENT>
      </FORMELEMENTGROUP>
      <FORMELEMENTGROUP>
        <FORMELEMENT>
          <NAME/>
          <DISPLAY>Use this form to modify or delete the address below.</DISPLAY>
          <TYPE>display</TYPE>
          <DEFAULT/>
          <CLASS/>
          <VTIP/>
          <REQUIRED/>
          <DISPLAYCLASS>formsubhead</DISPLAYCLASS>
          <SIZE/>
        </FORMELEMENT>
      </FORMELEMENTGROUP>
      <FORMELEMENTGROUP>
        <FORMELEMENT>
          <NAME/>
          <DISPLAY/>
          <TYPE>hr</TYPE>
          <DEFAULT/>
          <CLASS/>
          <VTIP/>
          <REQUIRED/>
          <DISPLAYCLASS>linebreak</DISPLAYCLASS>
          <SIZE/>
        </FORMELEMENT>
      </FORMELEMENTGROUP>
      <FORMELEMENTGROUP>
        <FORMELEMENT>
          <NAME></NAME>
          <DISPLAY>Address</DISPLAY>
          <TYPE>display</TYPE>
          <DEFAULT></DEFAULT>
          <CLASS></CLASS>
          <VTIP></VTIP>
          <REQUIRED></REQUIRED>
          <DISPLAYCLASS>col1b</DISPLAYCLASS>
          <SIZE></SIZE>
        </FORMELEMENT>
        <FORMELEMENT>
          <NAME>user_address</NAME>
          <DISPLAY></DISPLAY>
          <TYPE>text</TYPE>
          <DEFAULT></DEFAULT>
          <CLASS>input</CLASS>
          <VTIP>VARCHAR</VTIP>
          <REQUIRED>TRUE</REQUIRED>
          <DISPLAYCLASS>col2</DISPLAYCLASS>
          <SIZE>100</SIZE>
        </FORMELEMENT>
      </FORMELEMENTGROUP>
      <FORMELEMENTGROUP>
        <FORMELEMENT>
          <NAME></NAME>
          <DISPLAY>.</DISPLAY>
          <TYPE>display</TYPE>
          <DEFAULT></DEFAULT>
          <CLASS></CLASS>
          <VTIP></VTIP>
          <REQUIRED></REQUIRED>
          <DISPLAYCLASS>col1b white</DISPLAYCLASS>
          <SIZE></SIZE>
        </FORMELEMENT>
        <FORMELEMENT>
          <NAME>user_address_2</NAME>
          <DISPLAY></DISPLAY>
          <TYPE>text</TYPE>
          <DEFAULT></DEFAULT>
          <CLASS>input</CLASS>
          <VTIP>VARCHAR</VTIP>
          <REQUIRED>FALSE</REQUIRED>
          <DISPLAYCLASS>col2</DISPLAYCLASS>
          <SIZE>100</SIZE>
        </FORMELEMENT>
      </FORMELEMENTGROUP>
      <FORMELEMENTGROUP>
        <FORMELEMENT>
          <NAME></NAME>
          <DISPLAY>City</DISPLAY>
          <TYPE>display</TYPE>
          <DEFAULT></DEFAULT>
          <CLASS></CLASS>
          <VTIP></VTIP>
          <REQUIRED></REQUIRED>
          <DISPLAYCLASS>col1b</DISPLAYCLASS>
          <SIZE></SIZE>
        </FORMELEMENT>
        <FORMELEMENT>
          <NAME>user_city</NAME>
          <DISPLAY></DISPLAY>
          <TYPE>text</TYPE>
          <DEFAULT></DEFAULT>
          <CLASS>input</CLASS>
          <VTIP>VARCHAR</VTIP>
          <REQUIRED>TRUE</REQUIRED>
          <DISPLAYCLASS>col2</DISPLAYCLASS>
          <SIZE>100</SIZE>
        </FORMELEMENT>
      </FORMELEMENTGROUP>
      <FORMELEMENTGROUP id="user_state_select_div">
        <FORMELEMENT>
          <NAME></NAME>
          <DISPLAY>State</DISPLAY>
          <TYPE>display</TYPE>
          <DEFAULT></DEFAULT>
          <CLASS></CLASS>
          <VTIP></VTIP>
          <REQUIRED></REQUIRED>
          <DISPLAYCLASS>col1b</DISPLAYCLASS>
          <SIZE></SIZE>
        </FORMELEMENT>
        <FORMELEMENT>
          <NAME>user_state_select</NAME>
          <DISPLAY></DISPLAY>
          <TYPE>state</TYPE>
          <DEFAULT>Select One</DEFAULT>
          <CLASS>stateselect</CLASS>
          <VTIP>VARCHAR</VTIP>
          <REQUIRED>TRUE</REQUIRED>
          <DISPLAYCLASS>col2</DISPLAYCLASS>
          <SIZE>100</SIZE>
        </FORMELEMENT>
      </FORMELEMENTGROUP>
      <FORMELEMENTGROUP class="row reqs" id="user_state_text_div">
        <FORMELEMENT>
          <NAME></NAME>
          <DISPLAY>State</DISPLAY>
          <TYPE>display</TYPE>
          <DEFAULT></DEFAULT>
          <CLASS></CLASS>
          <VTIP></VTIP>
          <REQUIRED></REQUIRED>
          <DISPLAYCLASS>col1b</DISPLAYCLASS>
          <SIZE></SIZE>
        </FORMELEMENT>
        <FORMELEMENT>
          <NAME>user_state_text</NAME>
          <DISPLAY></DISPLAY>
          <TYPE>text</TYPE>
          <DEFAULT>Select One</DEFAULT>
          <CLASS>stateselect</CLASS>
          <VTIP>VARCHAR</VTIP>
          <REQUIRED>TRUE</REQUIRED>
          <DISPLAYCLASS>col2</DISPLAYCLASS>
          <SIZE>100</SIZE>
        </FORMELEMENT>
      </FORMELEMENTGROUP>
      <FORMELEMENTGROUP>
        <FORMELEMENT>
          <NAME></NAME>
          <DISPLAY>ZIP/Postal Code</DISPLAY>
          <TYPE>display</TYPE>
          <DEFAULT></DEFAULT>
          <CLASS></CLASS>
          <VTIP></VTIP>
          <REQUIRED></REQUIRED>
          <DISPLAYCLASS>col1b</DISPLAYCLASS>
          <SIZE></SIZE>
        </FORMELEMENT>
        <FORMELEMENT>
          <NAME>user_zip</NAME>
          <DISPLAY></DISPLAY>
          <TYPE>text</TYPE>
          <DEFAULT></DEFAULT>
          <CLASS>inputshort</CLASS>
          <VTIP>VARCHAR</VTIP>
          <REQUIRED>TRUE</REQUIRED>
          <DISPLAYCLASS>col2</DISPLAYCLASS>
          <SIZE>100</SIZE>
        </FORMELEMENT>
      </FORMELEMENTGROUP>
      <FORMELEMENTGROUP>
        <FORMELEMENT>
          <NAME></NAME>
          <DISPLAY>Country</DISPLAY>
          <TYPE>display</TYPE>
          <DEFAULT></DEFAULT>
          <CLASS></CLASS>
          <VTIP></VTIP>
          <REQUIRED></REQUIRED>
          <DISPLAYCLASS>col1b</DISPLAYCLASS>
          <SIZE></SIZE>
        </FORMELEMENT>
        <FORMELEMENT>
          <NAME>user_country</NAME>
          <DISPLAY></DISPLAY>
          <TYPE>country</TYPE>
          <DEFAULT selected="US">Select One</DEFAULT>
          <CLASS>stateselect</CLASS>
          <VTIP>VARCHAR</VTIP>
          <REQUIRED>TRUE</REQUIRED>
          <DISPLAYCLASS>col2</DISPLAYCLASS>
          <SIZE>100</SIZE>
        </FORMELEMENT>
      </FORMELEMENTGROUP>
      <FORMELEMENTGROUP>
        <FORMELEMENT>
          <NAME></NAME>
          <DISPLAY>Phone</DISPLAY>
          <TYPE>display</TYPE>
          <DEFAULT></DEFAULT>
          <CLASS></CLASS>
          <VTIP></VTIP>
          <REQUIRED></REQUIRED>
          <DISPLAYCLASS>col1b</DISPLAYCLASS>
          <SIZE></SIZE>
        </FORMELEMENT>
        <FORMELEMENT>
          <NAME>user_phone</NAME>
          <DISPLAY></DISPLAY>
          <TYPE>text</TYPE>
          <DEFAULT></DEFAULT>
          <CLASS>input</CLASS>
          <VTIP>notEmpty</VTIP>
          <REQUIRED>VNN</REQUIRED>
          <DISPLAYCLASS>col2</DISPLAYCLASS>
          <SIZE>100</SIZE>
        </FORMELEMENT>
      </FORMELEMENTGROUP>
      <FORMELEMENTGROUP>
        <FORMELEMENT>
          <NAME/>
          <DISPLAY/>
          <TYPE>hr</TYPE>
          <DEFAULT/>
          <CLASS/>
          <VTIP/>
          <REQUIRED/>
          <DISPLAYCLASS>linebreak_float</DISPLAYCLASS>
          <SIZE/>
        </FORMELEMENT>
      </FORMELEMENTGROUP>
      <FORMELEMENTGROUP>
        <FORMELEMENT>
          <NAME>submit</NAME>
          <DISPLAY/>
          <TYPE>submitimage</TYPE>
          <DEFAULT>/images/submit.gif</DEFAULT>
          <CLASS>submitter</CLASS>
          <VTIP/>
          <REQUIRED>FALSE</REQUIRED>
          <DISPLAYCLASS/>
          <SIZE/>
        </FORMELEMENT><FORMELEMENT>
          <NAME>delete</NAME>
          <DISPLAY/>
          <TYPE>submitimage</TYPE>
          <DEFAULT>/images/delete.gif</DEFAULT>
          <CLASS>submitter</CLASS>
          <VTIP/>
          <REQUIRED>FALSE</REQUIRED>
          <DISPLAYCLASS/>
          <SIZE/>
        </FORMELEMENT>
      </FORMELEMENTGROUP>
    </AFORM>
  </content>
</module>
