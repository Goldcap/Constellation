          <DISPLAYCLASS>col2</DISPLAYCLASS>
          <SIZE/>
        </FORMELEMENT>
      </FORMELEMENTGROUP>
      <FORMELEMENTGROUP>
        <FORMELEMENT>
          <NAME/>
          <DISPLAY>Password *</DISPLAY>
          <TYPE>display</TYPE>
          <DEFAULT/>
          <CLASS/>
          <VTIP/>
          <REQUIRED/>
          <DISPLAYCLASS>col1</DISPLAYCLASS>
          <SIZE/>
        </FORMELEMENT>
        <FORMELEMENT>
          <NAME>password</NAME>
          <DISPLAY/>
          <TYPE>password</TYPE>
          <DEFAULT/>
          <CLASS>input</CLASS>
          <VTIP>notEmpty</VTIP>
          <REQUIRED>TRUE</REQUIRED>
          <DISPLAYCLASS>col2</DISPLAYCLASS>
          <SIZE/>
        </FORMELEMENT>
      </FORMELEMENTGROUP>
      <FORMELEMENTGROUP>
        <FORMELEMENT>
          <NAME/>
          <DISPLAY/>
          <TYPE>hr</TYPE>
          <DEFAULT/>
