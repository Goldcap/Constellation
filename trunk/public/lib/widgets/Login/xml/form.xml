          <DEFAULT/>
          <CLASS>input</CLASS>
          <VTIP>notEmpty</VTIP>
          <REQUIRED>TRUE</REQUIRED>
          <DISPLAYCLASS>col2</DISPLAYCLASS>
          <SIZE/>
        </FORMELEMENT>
        <FORMELEMENT>
          <NAME>method</NAME>
