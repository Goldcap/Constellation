<map table="feedback" result="Feedback" allow_add="false">
  <column column="fk_question_id" name="Question" type="INTEGER" order="1" size="none,AnyProblems,HowHeard,Brought,Suggestions" format="enum"></column>
  <column column="fk_user_hash" name="User" type="VARCHAR" size="255" key="" order="2"></column>
  <column column="response" name="Response" type="LONGVARCHAR" size="100" key="" order="3"></column>
  <column column="feedback_id" type="INTEGER" size="11" key="PRI" order="4" hidden="true"></column>
  <criterion limit="0" distinct="true">
    <ascorderby column="feedback_id"></ascorderby>
    <!-- <descorderby column="column_name" />
    <criteria value="0" column="column_name"/>--></criterion>
  <pagination>
    <pagesperpage value="5"></pagesperpage>
    <recordssperpage value="5"></recordssperpage>
    <page value="1" var="page"></page>
    <sort var="sort"></sort>
  </pagination>
  <maplinks>
    <maplink column="Question" base="/feedback/detail/">
      <attribute name="feedback_id"></attribute>
    </maplink>
  </maplinks>
</map>
