<widget name="feedback" baseurl="/modules/feedback/">
  <content>
    <AFORM>
      <FORMNAME>feedback</FORMNAME>
      <FORMACTION>$self</FORMACTION>
      <JSVALIDATE>TRUE</JSVALIDATE>
      <FORMCLASS></FORMCLASS>
      <SERVERVALIDATE>TRUE</SERVERVALIDATE>
      <PASSTHROUGHVALUES>TRUE</PASSTHROUGHVALUES>
      <QUERYVALUES>TRUE</QUERYVALUES>
      <SHOWERRORS>FALSE</SHOWERRORS>
      <REQS>FALSE</REQS>
      <FORMELEMENTGROUP>
        <FORMELEMENT>
          <NAME>formhead</NAME>
          <DISPLAY>User Feedback</DISPLAY>
          <TYPE>display</TYPE>
          <DEFAULT></DEFAULT>
          <CLASS></CLASS>
          <VTIP></VTIP>
          <REQUIRED></REQUIRED>
          <DISPLAYCLASS>headline</DISPLAYCLASS>
          <SIZE></SIZE>
        </FORMELEMENT>
      </FORMELEMENTGROUP>
      <FORMELEMENTGROUP>
        <FORMELEMENT>
          <NAME>errornotice</NAME>
          <DISPLAY></DISPLAY>
          <TYPE>errornotice</TYPE>
          <DEFAULT>Please complete fields outlined in red.</DEFAULT>
          <CLASS>errornotice</CLASS>
          <VTIP></VTIP>
          <REQUIRED></REQUIRED>
          <DISPLAYCLASS>col2</DISPLAYCLASS>
          <SIZE></SIZE>
        </FORMELEMENT>
      </FORMELEMENTGROUP>
      <FORMELEMENTGROUP>
        <FORMELEMENT>
          <NAME></NAME>
          <DISPLAY></DISPLAY>
          <TYPE>hr</TYPE>
          <DEFAULT></DEFAULT>
          <CLASS></CLASS>
          <VTIP></VTIP>
          <REQUIRED></REQUIRED>
          <DISPLAYCLASS>linebreak</DISPLAYCLASS>
          <SIZE></SIZE>
        </FORMELEMENT>
      </FORMELEMENTGROUP>
      <FORMELEMENTGROUP>
        <FORMELEMENT>
          <NAME></NAME>
          <DISPLAY>Question</DISPLAY>
          <TYPE>display</TYPE>
          <DEFAULT></DEFAULT>
          <CLASS></CLASS>
          <VTIP></VTIP>
          <REQUIRED></REQUIRED>
          <DISPLAYCLASS>col1</DISPLAYCLASS>
          <SIZE></SIZE>
        </FORMELEMENT>
        <FORMELEMENT>
          <NAME>fk_question_id</NAME>
          <DISPLAY></DISPLAY>
          <TYPE>displaydb</TYPE>
          <DEFAULT table="feedback_question"></DEFAULT>
          <CLASS>input</CLASS>
          <VTIP>INTEGER</VTIP>
          <REQUIRED>TRUE</REQUIRED>
          <DISPLAYCLASS>col2</DISPLAYCLASS>
          <SIZE>11</SIZE>
        </FORMELEMENT>
      </FORMELEMENTGROUP>
      <FORMELEMENTGROUP>
        <FORMELEMENT>
          <NAME></NAME>
          <DISPLAY>User</DISPLAY>
          <TYPE>display</TYPE>
          <DEFAULT></DEFAULT>
          <CLASS></CLASS>
          <VTIP></VTIP>
          <REQUIRED></REQUIRED>
          <DISPLAYCLASS>col1</DISPLAYCLASS>
          <SIZE></SIZE>
        </FORMELEMENT>
        <FORMELEMENT>
          <NAME>fk_user_hash</NAME>
          <DISPLAY></DISPLAY>
          <TYPE>display</TYPE>
          <DEFAULT></DEFAULT>
          <CLASS>input</CLASS>
          <VTIP>VARCHAR</VTIP>
          <REQUIRED>TRUE</REQUIRED>
          <DISPLAYCLASS>col2</DISPLAYCLASS>
          <SIZE>255</SIZE>
        </FORMELEMENT>
      </FORMELEMENTGROUP>
      <FORMELEMENTGROUP>
        <FORMELEMENT>
          <NAME></NAME>
          <DISPLAY>Response</DISPLAY>
          <TYPE>display</TYPE>
          <DEFAULT></DEFAULT>
          <CLASS></CLASS>
          <VTIP></VTIP>
          <REQUIRED></REQUIRED>
          <DISPLAYCLASS>col1</DISPLAYCLASS>
          <SIZE></SIZE>
        </FORMELEMENT>
        <FORMELEMENT>
          <NAME>response</NAME>
          <DISPLAY></DISPLAY>
          <TYPE>display</TYPE>
          <DEFAULT></DEFAULT>
          <CLASS>inputarea</CLASS>
          <VTIP>LONGVARCHAR</VTIP>
          <REQUIRED>TRUE</REQUIRED>
          <DISPLAYCLASS>col2</DISPLAYCLASS>
          <SIZE></SIZE>
        </FORMELEMENT>
      </FORMELEMENTGROUP>
      <FORMELEMENTGROUP>
        <FORMELEMENT>
          <NAME></NAME>
          <DISPLAY></DISPLAY>
          <TYPE>hr</TYPE>
          <DEFAULT></DEFAULT>
          <CLASS></CLASS>
          <VTIP></VTIP>
          <REQUIRED></REQUIRED>
          <DISPLAYCLASS>linebreak_float</DISPLAYCLASS>
          <SIZE></SIZE>
        </FORMELEMENT>
      </FORMELEMENTGROUP>
      <FORMELEMENTGROUP>
        <FORMELEMENT>
          <NAME>feedback_id</NAME>
          <DISPLAY></DISPLAY>
          <TYPE>hidden</TYPE>
          <DEFAULT></DEFAULT>
          <CLASS>input</CLASS>
          <VTIP>INTEGER</VTIP>
          <REQUIRED>TRUE</REQUIRED>
          <DISPLAYCLASS>col2</DISPLAYCLASS>
          <SIZE>11</SIZE>
        </FORMELEMENT>
      </FORMELEMENTGROUP>
    </AFORM>
  </content>
</widget>
