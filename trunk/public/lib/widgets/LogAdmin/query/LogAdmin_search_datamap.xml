<map title="Flash Log Data" table="log_flashlog" result="log" allow_add="false">
  <column column="flashlog_ticket" name="User Ticket" type="VARCHAR" size="255" key="" order="1"></column>
  <column column="flashlog_message" name="Message" type="VARCHAR" size="255" key="" order="2" util="formatFlashMessage"></column>
  <column column="flashlog_date" name="Date" type="TIMESTAMP" size="" key="" order="3"></column>
  <column column="flashlog_port" type="INTEGER" size="11" key="" order="4" hidden="true"></column>
  <column column="flashlog_error" type="TINYINT" size="4" key="" order="5" util="formatFlashError"></column>
  <column column="flashlog_id" type="INTEGER" size="11" key="PRI" order="6" hidden="true"></column>
  <criterion limit="0" distinct="true">
    <descorderby column="flashlog_id"></descorderby>
    <criteria scope="PROCESS" value="num" column="flashlog_id" key="maingroup" type="OR" />
    <criteria scope="PROCESS" value="search" column="flashlog_ticket" key="maingroup" constant="LIKE" type="OR" />
    <criteria scope="PROCESS" value="search" column="flashlog_message" key="maingroup" constant="LIKE" type="OR" last="true" />
    <criteria scope="GET" value="errors" column="flashlog_error" constant="GREATER_EQUAL" />
    <!-- <descorderby column="column_name" />
    <criteria value="0" column="column_name"/>--></criterion>
  <pagination>
    <pagesperpage value="5"></pagesperpage>
    <recordssperpage value="50"></recordssperpage>
    <page value="1" var="page"></page>
    <sort var="sort"></sort>
  </pagination>
  <maplinks>
    <maplink column="User Ticket" base="/log/detail/">
      <attribute name="flashlog_id"></attribute>
    </maplink>
  </maplinks>
</map>