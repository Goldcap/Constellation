          <NAME></NAME>
          <DISPLAY>Find an Order: </DISPLAY>
          <TYPE>display</TYPE>
          <DEFAULT></DEFAULT>
          <CLASS></CLASS>
          <VTIP></VTIP>
          <REQUIRED></REQUIRED>
          <DISPLAYCLASS>col1</DISPLAYCLASS>
          <SIZE></SIZE>
        </FORMELEMENT>
        <FORMELEMENT>
          <NAME>order_search</NAME>
          <DISPLAY></DISPLAY>
          <TYPE>text</TYPE>
          <DEFAULT></DEFAULT>
          <CLASS>input</CLASS>
          <VTIP>notEmpty</VTIP>
          <REQUIRED>TRUE</REQUIRED>
          <DISPLAYCLASS>col2</DISPLAYCLASS>
          <SIZE>255</SIZE>
        </FORMELEMENT>
        <FORMELEMENT>
          <NAME>search</NAME>
          <DISPLAY></DISPLAY>
          <TYPE>submitimage</TYPE>
          <DEFAULT>/images/submit.gif</DEFAULT>
          <CLASS></CLASS>
          <VTIP></VTIP>
          <REQUIRED>FALSE</REQUIRED>
          <DISPLAYCLASS></DISPLAYCLASS>
          <SIZE></SIZE>
        </FORMELEMENT>
        <FORMELEMENT>
          <NAME>search_help</NAME>
          <DISPLAY/>
