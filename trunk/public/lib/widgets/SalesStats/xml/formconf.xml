          <DEFAULT>/images/Neu/16x16/status/dialog-question.png</DEFAULT>
          <CLASS>icon16</CLASS>
          <VTIP></VTIP>
          <REQUIRED></REQUIRED>
          <DISPLAYCLASS>col2c</DISPLAYCLASS>
          <SIZE/>
        </FORMELEMENT>
        <FORMELEMENT>
          <NAME>search_help</NAME>
          <DISPLAY>Enter any information about this order: email, name, credit card, or date.</DISPLAY>
          <TYPE species="toolTip">dojo</TYPE>
          <DEFAULT/>
          <CLASS></CLASS>
          <VTIP></VTIP>
          <REQUIRED></REQUIRED>
          <DISPLAYCLASS></DISPLAYCLASS>
          <SIZE/>
        </FORMELEMENT>
      </FORMELEMENTGROUP>
      <FORMELEMENTGROUP>
        <FORMELEMENT>
          <NAME></NAME>
          <DISPLAY></DISPLAY>
          <TYPE>hr</TYPE>
          <DEFAULT></DEFAULT>
          <CLASS></CLASS>
          <VTIP></VTIP>
          <REQUIRED></REQUIRED>
          <DISPLAYCLASS>twocol</DISPLAYCLASS>
          <SIZE></SIZE>
        </FORMELEMENT>
      </FORMELEMENTGROUP>
      <FORMELEMENTGROUP>
        <FORMELEMENT>
          <NAME>orders</NAME>
          <DISPLAY></DISPLAY>
          <TYPE>html</TYPE>
          <DEFAULT></DEFAULT>
          <CLASS>input</CLASS>
          <VTIP>oneChar</VTIP>
          <REQUIRED>FALSE</REQUIRED>
          <DISPLAYCLASS>twocol</DISPLAYCLASS>
          <SIZE>255</SIZE>
        </FORMELEMENT>
      </FORMELEMENTGROUP>
      <FORMELEMENTGROUP>
        <FORMELEMENT>
          <NAME>errornotice</NAME>
          <DISPLAY></DISPLAY>
          <TYPE>errornotice</TYPE>
          <DEFAULT>Please complete fields outlined in red.</DEFAULT>
          <CLASS>errornotice</CLASS>
          <VTIP></VTIP>
          <REQUIRED></REQUIRED>
          <DISPLAYCLASS>col2</DISPLAYCLASS>
          <SIZE></SIZE>
        </FORMELEMENT>
      </FORMELEMENTGROUP>
