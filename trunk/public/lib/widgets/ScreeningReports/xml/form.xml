<widget name="DailyReport" baseurl="/modules/UserAdmin/">
  <content>
    <AFORM>
      <FORMNAME>DailyReport</FORMNAME>
      <FORMACTION noenc="true" raw="true">$self</FORMACTION>
      <FORMMETHOD>POST</FORMMETHOD>
      <JSVALIDATE>TRUE</JSVALIDATE>
      <FORMCLASS>orders</FORMCLASS>
      <SERVERVALIDATE>TRUE</SERVERVALIDATE>
      <PASSTHROUGHVALUES>TRUE</PASSTHROUGHVALUES>
      <QUERYVALUES>TRUE</QUERYVALUES>
      <SHOWERRORS>FALSE</SHOWERRORS>
      <REQS>FALSE</REQS>
      <FORMELEMENTGROUP class="row padded_top">
        <FORMELEMENT>
          <NAME></NAME>
          <DISPLAY>Start Date:</DISPLAY>
          <TYPE>display</TYPE>
          <DEFAULT></DEFAULT>
          <CLASS></CLASS>
          <VTIP></VTIP>
          <REQUIRED></REQUIRED>
          <DISPLAYCLASS>col1</DISPLAYCLASS>
          <SIZE></SIZE>
        </FORMELEMENT>
        <FORMELEMENT>
          <NAME>startdate</NAME>
          <DISPLAY></DISPLAY>
          <TYPE>jquery_anytime</TYPE>
          <DEFAULT></DEFAULT>
          <CLASS>input</CLASS>
          <VTIP>TIMESTAMP</VTIP>
          <REQUIRED>FALSE</REQUIRED>
          <DISPLAYCLASS>col2</DISPLAYCLASS>
          <SIZE>"%Y-%m-%d"</SIZE>
        </FORMELEMENT>
        <FORMELEMENT>
          <NAME>search</NAME>
          <DISPLAY></DISPLAY>
          <TYPE>submitimage</TYPE>
          <DEFAULT>/images/submit.gif</DEFAULT>
          <CLASS></CLASS>
          <VTIP></VTIP>
          <REQUIRED>FALSE</REQUIRED>
          <DISPLAYCLASS></DISPLAYCLASS>
          <SIZE></SIZE>
        </FORMELEMENT>
      </FORMELEMENTGROUP>
      <FORMELEMENTGROUP>
        <FORMELEMENT>
          <NAME></NAME>
          <DISPLAY>End Date:</DISPLAY>
          <TYPE>display</TYPE>
          <DEFAULT></DEFAULT>
          <CLASS></CLASS>
          <VTIP></VTIP>
          <REQUIRED></REQUIRED>
          <DISPLAYCLASS>col1</DISPLAYCLASS>
          <SIZE></SIZE>
        </FORMELEMENT>
        <FORMELEMENT>
          <NAME>enddate</NAME>
          <DISPLAY></DISPLAY>
          <TYPE>jquery_anytime</TYPE>
          <DEFAULT></DEFAULT>
          <CLASS>input</CLASS>
          <VTIP>TIMESTAMP</VTIP>
          <REQUIRED>FALSE</REQUIRED>
          <DISPLAYCLASS>col2</DISPLAYCLASS>
          <SIZE>"%Y-%m-%d"</SIZE>
        </FORMELEMENT>
      </FORMELEMENTGROUP>
      <FORMELEMENTGROUP>
        <FORMELEMENT>
          <NAME></NAME>
          <DISPLAY></DISPLAY>
          <TYPE>hr</TYPE>
          <DEFAULT></DEFAULT>
          <CLASS></CLASS>
          <VTIP></VTIP>
          <REQUIRED></REQUIRED>
          <DISPLAYCLASS>linebreak_float</DISPLAYCLASS>
          <SIZE></SIZE>
        </FORMELEMENT>
      </FORMELEMENTGROUP>
      <FORMELEMENTGROUP>
        <FORMELEMENT>
          <NAME>errornotice</NAME>
          <DISPLAY></DISPLAY>
          <TYPE>errornotice</TYPE>
          <DEFAULT>Please complete fields outlined in red.</DEFAULT>
          <CLASS>errornotice</CLASS>
          <VTIP></VTIP>
          <REQUIRED></REQUIRED>
          <DISPLAYCLASS>col2</DISPLAYCLASS>
          <SIZE></SIZE>
        </FORMELEMENT>
      </FORMELEMENTGROUP>
    </AFORM>
  </content>
</widget>
