      </FORMELEMENTGROUP>
      <FORMELEMENTGROUP>
        <FORMELEMENT>
          <NAME></NAME>
          <DISPLAY></DISPLAY>
          <TYPE>hr</TYPE>
          <DEFAULT></DEFAULT>
          <CLASS></CLASS>
          <VTIP></VTIP>
          <REQUIRED></REQUIRED>
          <DISPLAYCLASS>linebreak_float</DISPLAYCLASS>
          <SIZE></SIZE>
        </FORMELEMENT>
      </FORMELEMENTGROUP>
      <FORMELEMENTGROUP>
        <FORMELEMENT>
          <NAME>pre_user_id</NAME>
          <DISPLAY></DISPLAY>
          <TYPE>hidden</TYPE>
          <DEFAULT></DEFAULT>
          <CLASS>input</CLASS>
          <VTIP>INTEGER</VTIP>
          <REQUIRED>TRUE</REQUIRED>
          <DISPLAYCLASS>col2</DISPLAYCLASS>
          <SIZE>11</SIZE>
        </FORMELEMENT>
      </FORMELEMENTGROUP>
    </AFORM>
  </content>
</widget>
