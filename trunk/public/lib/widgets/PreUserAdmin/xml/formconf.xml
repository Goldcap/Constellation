<widget name="pre_user" baseurl="/modules/pre_user/">
  <content>
    <AFORM>
      <FORMNAME>pre_user</FORMNAME>
      <FORMACTION>$self</FORMACTION>
      <JSVALIDATE>TRUE</JSVALIDATE>
      <FORMCLASS></FORMCLASS>
      <SERVERVALIDATE>TRUE</SERVERVALIDATE>
      <PASSTHROUGHVALUES>TRUE</PASSTHROUGHVALUES>
      <QUERYVALUES>TRUE</QUERYVALUES>
      <SHOWERRORS>FALSE</SHOWERRORS>
      <REQS>FALSE</REQS>
      <FORMELEMENTGROUP>
        <FORMELEMENT>
          <NAME>formhead</NAME>
          <DISPLAY>User Pre-Registration</DISPLAY>
          <TYPE>display</TYPE>
          <DEFAULT></DEFAULT>
          <CLASS></CLASS>
          <VTIP></VTIP>
          <REQUIRED></REQUIRED>
          <DISPLAYCLASS>headline</DISPLAYCLASS>
          <SIZE></SIZE>
        </FORMELEMENT>
        <FORMELEMENT>
          <NAME>requirenotice</NAME>
          <DISPLAY>* required fields</DISPLAY>
          <TYPE>display</TYPE>
          <DEFAULT></DEFAULT>
          <CLASS></CLASS>
          <VTIP></VTIP>
          <REQUIRED></REQUIRED>
          <DISPLAYCLASS>reqmessage</DISPLAYCLASS>
          <SIZE></SIZE>
        </FORMELEMENT>
      </FORMELEMENTGROUP>
      <FORMELEMENTGROUP>
        <FORMELEMENT>
          <NAME>errornotice</NAME>
          <DISPLAY></DISPLAY>
          <TYPE>errornotice</TYPE>
          <DEFAULT>Please complete fields outlined in red.</DEFAULT>
          <CLASS>errornotice</CLASS>
          <VTIP></VTIP>
          <REQUIRED></REQUIRED>
          <DISPLAYCLASS>col2</DISPLAYCLASS>
          <SIZE></SIZE>
        </FORMELEMENT>
      </FORMELEMENTGROUP>
      <FORMELEMENTGROUP>
        <FORMELEMENT>
          <NAME></NAME>
          <DISPLAY></DISPLAY>
          <TYPE>hr</TYPE>
          <DEFAULT></DEFAULT>
          <CLASS></CLASS>
          <VTIP></VTIP>
          <REQUIRED></REQUIRED>
          <DISPLAYCLASS>linebreak</DISPLAYCLASS>
          <SIZE></SIZE>
        </FORMELEMENT>
      </FORMELEMENTGROUP>
      <FORMELEMENTGROUP>
        <FORMELEMENT>
          <NAME></NAME>
          <DISPLAY>User Email</DISPLAY>
          <TYPE>display</TYPE>
          <DEFAULT></DEFAULT>
          <CLASS></CLASS>
          <VTIP></VTIP>
          <REQUIRED></REQUIRED>
          <DISPLAYCLASS>col1</DISPLAYCLASS>
          <SIZE></SIZE>
        </FORMELEMENT>
        <FORMELEMENT>
          <NAME>pre_user_email</NAME>
          <DISPLAY></DISPLAY>
          <TYPE>display</TYPE>
          <DEFAULT></DEFAULT>
          <CLASS>input</CLASS>
          <VTIP>VARCHAR</VTIP>
          <REQUIRED>TRUE</REQUIRED>
          <DISPLAYCLASS>col2</DISPLAYCLASS>
          <SIZE>255</SIZE>
        </FORMELEMENT>
      </FORMELEMENTGROUP>
      <FORMELEMENTGROUP>
        <FORMELEMENT>
          <NAME></NAME>
          <DISPLAY>User Date Added</DISPLAY>
          <TYPE>display</TYPE>
          <DEFAULT></DEFAULT>
          <CLASS></CLASS>
          <VTIP></VTIP>
          <REQUIRED></REQUIRED>
          <DISPLAYCLASS>col1</DISPLAYCLASS>
          <SIZE></SIZE>
        </FORMELEMENT>
        <FORMELEMENT>
          <NAME>pre_user_date_added</NAME>
          <DISPLAY></DISPLAY>
          <TYPE>display</TYPE>
          <DEFAULT></DEFAULT>
          <CLASS>input</CLASS>
          <VTIP>TIMESTAMP</VTIP>
          <REQUIRED>TRUE</REQUIRED>
          <DISPLAYCLASS>col2</DISPLAYCLASS>
          <SIZE></SIZE>
        </FORMELEMENT>
      </FORMELEMENTGROUP>
      <FORMELEMENTGROUP>
        <FORMELEMENT>
          <NAME></NAME>
          <DISPLAY>User Status</DISPLAY>
          <TYPE>display</TYPE>
          <DEFAULT></DEFAULT>
          <CLASS></CLASS>
          <VTIP></VTIP>
          <REQUIRED></REQUIRED>
          <DISPLAYCLASS>col1</DISPLAYCLASS>
          <SIZE></SIZE>
        </FORMELEMENT>
        <FORMELEMENT>
          <NAME>preuser_status</NAME>
          <DISPLAY></DISPLAY>
          <TYPE>selectdisplay</TYPE>
          <DEFAULT>Select One</DEFAULT>
          <CLASS>input</CLASS>
          <VTIP>0</VTIP>
          <REQUIRED>TRUE</REQUIRED>
          <DISPLAYCLASS>col2</DISPLAYCLASS>
          <SIZE>4</SIZE>
            <VALUE value="approved">Approved</VALUE>
            <VALUE value="not-approved">Not Approved</VALUE>
        </FORMELEMENT>
      </FORMELEMENTGROUP>
      <FORMELEMENTGROUP>
        <FORMELEMENT>
          <NAME></NAME>
          <DISPLAY>User Code</DISPLAY>
          <TYPE>display</TYPE>
          <DEFAULT></DEFAULT>
          <CLASS></CLASS>
          <VTIP></VTIP>
          <REQUIRED></REQUIRED>
          <DISPLAYCLASS>col1</DISPLAYCLASS>
          <SIZE></SIZE>
        </FORMELEMENT>
        <FORMELEMENT>
          <NAME>pre_user_code</NAME>
          <DISPLAY></DISPLAY>
          <TYPE>display</TYPE>
          <DEFAULT></DEFAULT>
          <CLASS>input</CLASS>
          <VTIP>VARCHAR</VTIP>
          <REQUIRED>TRUE</REQUIRED>
