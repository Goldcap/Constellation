<map table="screening" result="screening_list_datamap">
  <column column="screening_name" type="VARCHAR" size="" key="" order="1"></column>
  <column column="screening_date" type="DATE" size="" key="" order="1"></column>
  <column column="screening_time" type="TIME" size="" key="" order="2"></column>
  <column column="screening_post_message" type="LONGVARCHAR" size="" key="" order="3"></column>
  <column column="screening_paid_status" type="TINYINT" size="1" key="" order="4"></column>
  <column column="screening_seats_occupied" type="INTEGER" size="11" key="" order="5"></column>
  <column column="screening_created_at" type="TIMESTAMP" size="" key="" order="6"></column>
  <column column="screening_updated_at" type="TIMESTAMP" size="" key="" order="7"></column>
  <column column="screening_unique_key" type="VARCHAR" size="15" key="" order="8"></column>
  <column column="screening_status" type="TINYINT" size="1" key="" order="9"></column>
  <column column="screening_type" type="TINYINT" size="1" key="" order="10"></column>
  <column column="screening_total_seats" type="INTEGER" size="11" key="" order="11"></column>
  <column column="screening_constellation_image" type="VARCHAR" size="255" key="" order="12"></column>
  <column column="screening_guest_name" type="VARCHAR" size="255" key="" order="13"></column>
  <column column="screening_guest_image" type="VARCHAR" size="255" key="" order="14"></column>
  <column column="screening_description" type="LONGVARCHAR" size="" key="" order="15"></column>
  <column column="screening_live_webcam" type="TINYINT" size="1" key="" order="16"></column>
  <column column="screening_is_admin" type="TINYINT" size="1" key="" order="17"></column>
  <column column="screening_featured" type="TINYINT" size="1" key="" order="18"></column>
  <column column="screening_credit_status" type="TINYINT" size="1" key="" order="19"></column>
  <column column="screening_default_timezone" type="VARCHAR" size="255" key="" order="20"></column>
  <column column="screening_receipt_status" type="TINYINT" size="1" key="" order="21"></column>
  <column column="screening_default_timezone_id" type="VARCHAR" size="255" key="" order="22"></column>
  <column column="screening_video_server_hostname" type="VARCHAR" size="120" key="" order="23"></column>
  <column column="screening_video_server_instance_id" type="VARCHAR" size="16" key="" order="24"></column>
  <column column="screening_is_private" type="TINYINT" size="1" key="" order="25"></column>
  <column column="fk_host_id" type="INTEGER" size="11" key="MUL" order="26"></column>
  <column column="fk_film_id" type="INTEGER" size="11" key="MUL" order="27"></column>
  <column column="fk_payment_id" type="INTEGER" size="11" key="MUL" order="27"></column>
  <column column="screening_id" type="INTEGER" size="11" key="PRI" order="28"></column>
  <join foreigntable="film" local="fk_film_id" foreign="film_id" left="false">
    <foreigncolumn column="film_id" type="INTEGER" size="10" key="" order="13"></foreigncolumn>
    <foreigncolumn column="film_setup_price" name="screening_film_setup_price" type="FLOAT" size="10" key="" order="13" format="%.02f"></foreigncolumn>
    <foreigncolumn column="film_name" name="screening_film_name" type="VARCHAR" size="10" key="" order="13" ></foreigncolumn>
  </join>
  <criterion limit="0" distinct="true">
    <criteria scope="PROCESS" value="screening_unique_key" column="screening_unique_key" />
    <ascorderby column="screening_id"></ascorderby>
    <!-- <descorderby column="column_name" />
    <criteria value="0" column="column_name"/>--></criterion>
  <pagination>
    <pagesperpage value="5"></pagesperpage>
    <recordssperpage value="1"></recordssperpage>
    <page value="1" var="page"></page>
    <sort var="sort"></sort>
  </pagination>
</map>
