<?xml version="1.0" encoding="UTF-8"?>
<validators>
	<validator>
		<type>notEmpty</type>
		<expression><![CDATA[/\S/]]></expression>
	</validator>
	<validator>
		<type>oneChar</type>
		<expression><![CDATA[/[A-Za-z]{1}/]]></expression>
	</validator>
	<validator>
		<type>twoChar</type>
		<expression><![CDATA[/[A-Za-z]{2}/]]></expression>
	</validator>
	<validator>
		<type>oneLen</type>
		<expression><![CDATA[/[A-Za-z0-9]{1}/]]></expression>
	</validator>
	<validator>
		<type>money</type>
		<expression><![CDATA[/^([0-9]{1,255})?(\.)?[0-9]{1,2}$/]]></expression>
	</validator>
	<validator>
		<type>LONGVARCHAR</type>
		<expression><![CDATA[/\S/]]></expression>
	</validator>
	<validator>
		<type>VARCHAR</type>
		<expression><![CDATA[/\S/]]></expression>
	</validator>
		<validator>
		<type>CHAR</type>
		<expression><![CDATA[/\S/]]></expression>
	</validator>
	<validator>
		<type>FLOAT</type>
		<expression><![CDATA[/^([0-9]{1,255})?(\.)?[0-9]{1,2}$/]]></expression>
	</validator>
	<validator>
		<type>FLOATFORCE</type>
		<expression><![CDATA[/^([0-9]{1,255})?(\.)[0-9]{1,2}$/]]></expression>
	</validator>
	<validator>
		<type>INTEGER</type>
		<expression><![CDATA[/[0-9]/]]></expression>
	</validator>
  <validator>
		<type>TINYINT</type>
		<expression><![CDATA[/[0-9]/]]></expression>
	</validator>
	<validator>
		<type>TIMESTAMP</type>
		<expression><![CDATA[/\d{4}-\d{2}-\d{2} \d{2}:\d{2}:\d{2}/]]></expression>
	</validator>
	<validator>
		<type>FALSE</type>
		<expression><![CDATA[/^(.*)/]]></expression>
	</validator>
	<validator>
		<type>phoneNumber</type>
		<expression><![CDATA[/^\d{10}/]]></expression>
	</validator>
	<validator>
		<type>phonearea</type>
		<expression><![CDATA[/^\d{3}/]]></expression>
	</validator>
	<validator>
		<type>phoneprefix</type>
		<expression><![CDATA[/^\d{3}/]]></expression>
	</validator>
	<validator>
		<type>phonesuffix</type>
		<expression><![CDATA[/^\d{4}/]]></expression>
	</validator>
	<validator>
		<type>zipCode</type>
		<expression><![CDATA[/^\d{5}([\-]\d{4})?$/]]></expression>
	</validator>
	<validator>
		<type>Currency</type>
		<expression><![CDATA[/\$\d{1,3}(,\d{3})*\.\d{2}/]]></expression>
	</validator>
	<validator>
		<type>Time</type>
		<expression><![CDATA[/^([1-9]|1[0-2]):[0-5]\d$/]]></expression>
	</validator>
	<validator>
		<type>email</type>
		<expression><![CDATA[/^.+\@(\[?)[a-zA-Z0-9\-\.]+\.([a-zA-Z]{2,4}|[0-9]{1,3})(\]?)$/]]></expression>
	</validator>
	<validator>
		<type>phoneNumberInternational</type>
		<expression><![CDATA[/^\d(\d|-){7,20}/]]></expression>
	</validator>
	<validator>
		<type>ipAddress</type>
		<expression><![CDATA[/^((25[0-5]|2[0-4][0-9]|1[0-9][0-9]|[1-9][0-9]|[0-9])\.){3}(25[0-5]|2[0-4][0-9]|1[0-9][0-9]|[1-9][0-9]|[0-9])$/]]></expression>
	</validator>
	<validator>
		<type>Date</type>
		<expression><![CDATA[/^\d{1,2}(\-|\/|\.)\d{1,2}\1\d{4}$/]]></expression>
	</validator>
	<validator>
		<type>DateTime</type>
		<expression><![CDATA[/^([0-9]{2,4})-([0-1][0-9])-([0-3][0-9]) (?:([0-2][0-9]):([0-5][0-9]):([0-5][0-9]))?$/]]></expression>
	</validator>
	<validator>
		<type>State</type>
		<expression><![CDATA[/^(AK|AL|AR|AZ|CA|CO|CT|DC|DE|FL|GA|HI|IA|ID|IL|IN|KS|KY|LA|MA|MD|ME|MI|MN|MO|MS|MT|NB|NC|ND|NH|NJ|NM|NV|NY|OH|OK|OR|PA|RI|SC|SD|TN|TX|UT|VA|VT|WA|WI|WV|WY)$/i]]></expression>
	</validator>
	<validator>
		<type>SSN</type>
		<expression><![CDATA[/^\d{3}\-\d{2}\-\d{4}$/]]></expression>
	</validator>
	<validator>
		<type>truncURL</type>
		<expression><![CDATA[/([\w-]+\.)+[\w-]+(\/[\w- .\/?%~?!#-_&=]*)?$/]]></expression>
	</validator>
	<validator>
		<type>URL</type>
		<expression><![CDATA[/http(s)?:\/\/([\w-]+\.)+[\w-]+(\/[\w- .\/?%~?!#-_&=]*)?$/]]></expression>
	</validator>
	<validator>
		<type>truncURL</type>
		<expression><![CDATA[/([\w-]+\.)+[\w-]+(\/[\w- .\/?%~?!#-_&=]*)?$/]]></expression>
	</validator>
	<validator>
		<type>YouTubePlaylistURL</type>
		<expression><![CDATA[/^http:\/\/www.youtube.com\/view_play_list\?p=([0-9A-Z]{16})$/]]></expression>
	</validator>
	<validator>
		<type>CreditCard</type>
		<expression><![CDATA[/^(?:4[0-9]{12}(?:[0-9]{3})?|5[1-5][0-9]{14}|6(?:011|5[0-9][0-9])[0-9]{12}|3[47][0-9]{13}|3(?:0[0-5]|[68][0-9])[0-9]{11}|(?:2131|1800|35\d{3})\d{11})$/]]></expression>
	</validator>
</validators>    
